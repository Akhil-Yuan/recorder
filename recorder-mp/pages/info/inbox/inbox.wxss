}
.infoList{
  background-color: #ffffff;
}
.van-swipe-cell{
  height: 130rpx;
  line-height: 130rpx;
  margin: 0 25rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.075);
}
.title{
  font-weight: 600;
}
.time{
  float: right;
  margin-right: 25rpx;
  color: rgba(0, 0, 0, 0.5);
}
.van-swipe-cell__right {
  display: inline-block;
  text-align: center;
  width: 140rpx;
  height: 130rpx;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
